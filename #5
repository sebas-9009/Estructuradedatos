#include "stdafx.h"

#include <iostream>

#include "conio.h"



using namespace std; 



int main ()

{

    int a, b, c=0, aux;

    int b2=0, aux2=0;

    cout << "Ingrese un numero" << endl;

    cin >> a;

    aux = a;

    while (a > 0)

    {

        b = a % 10;

        b = 9 - b;

        c = (b + c) * 10;

        a = a / 10;

    }

    c = c / 10;

    while (c > 0)

    {

        b2 = c % 10;

        aux2 = (b2 + aux2) * 10;

        c = c / 10;

    }

	cout <<"El complemento 9 es :"<< endl;

    cout << aux2 / 10 << endl;

    getch();

    return 0;

}
